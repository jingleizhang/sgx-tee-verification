{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "g++ build main",
            "command": "g++",
            //"command": "pwd",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "-std=c++11",
                "-Wall",
                "-loauthcpp",
                "-I${workspaceFolder}/liboauth",
                "-L${workspaceFolder}/liboauth",
                "${workspaceFolder}/mongoose.o",
                "${workspaceFolder}/main.cpp",
                "${workspaceFolder}/main.cpp",
                "-o",
                "${workspaceFolder}/main",
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build",
            "detail": ""
        }
    ],
    "version": "2.0.0"
}